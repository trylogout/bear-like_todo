rn this.todos.filter((todo) => !todo.completed);\n      }\n    },\n  },\n  methods: {\n    removeTodo(id) {\n      this.todos = this.todos.filter((item) => item.id !== id);\n    },\n    addToDoItem(todo) {\n      this.todos.push(todo);\n    },\n    filteredToDos(value) {\n      switch (value) {\n        case \"all\":\n          return this.todos;\n        case \"completed\":\n          return this.todos.filter((todo) => todo.completed);\n        case \"notCompleted\":\n          return this.todos.filter((todo) => !todo.completed);\n      }\n    },\n  },\n  components: {\n    ToDoList,\n    AddToDoComponent,\n    Loader,\n  },\n};\n</script>\n<style lang=\"scss\">\n@import 'https://use.fontawesome.com/releases/v5.0.10/css/all.css';\n@import 'https://fonts.googleapis.com/css?family=Lato:300';\n\n.container {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  margin-left: -64.5px;\n  margin-top: -85.5px;\n}\n\n.ghost {\n  animation: float 3s ease-out infinite;\n}\n\n@keyframes float {\n  50% {\n    transform: translate(0, 20px);\n  }\n}\n.shadowFrame {\n  width: 130px;\n  margin-top: 15px;\n}\n.shadow {\n  animation: shrink 3s ease-out infinite;\n  transform-origin: center center;\n  ellipse {\n    transform-origin: center center;\n  }\n}\n\n@keyframes shrink {\n  0% {\n    width: 90%;\n    margin: 0 5%;\n  }\n  50% {\n    width: 60%;\n    margin: 0 18%;\n  }\n  100% {\n    width: 90%;\n    margin: 0 5%;\n  }\n}\n\n.sadText {\n  color: #b9bec3;\n  top: 30%;\n  left: 50%;\n  position: absolute;\n  margin-left: -94.5px;\n  margin-top: -85.5px;\n}\n.upper {\n  font-size: 28px;\n}\n.lower {\n  font-size: 16px;\n}\n\n/* -------------- */\n\n.panel {\n\tmargin: 15px auto 15px;\n  max-width: 250px;\n  position: absolute;\n  top: 0px;\n  right: 25px;\n}\n\n.admin-panel {\n\tpadding: 12px 0;\n}\n\n.admin-panel .text {\n\tcolor: #888888;\n\tfont-size: 13px;\n\tfont-weight: bold;\n\ttext-transform: uppercase;\n\ttext-decoration: none;\n\tmargin-left: 10px;\n}\n\n.admin-panel .f